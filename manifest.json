{
  "manifest_version": 3,
  "name": "Kite Filter",
  "description": "Private & Secure Web-Filter",
  "version": "0.0.8",

  "permissions": [
    "activeTab",
    "alarms",
    "bookmarks",
    "history",
    "identity",
    "storage",
    "tabs",
    "webNavigation"
  ],

  "background": {
    "service_worker": "js/filter.js"
  },

  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["js/inject.js"]
    }]
} 