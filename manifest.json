{
    "manifest_version": 3,
    "name": "Kite Filter",
    "description": "Private & Secure Web-Filter",
    "version": "0.0.2",

    "permissions": [
      "bookmarks",
      "alarms",
      "storage",
      "identity",
      "history",
      "activeTab"
    ],

    "background": {
      "service_worker": "js/filter.js"
    },

    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["js/inject.js"]
      }
    ]
} 